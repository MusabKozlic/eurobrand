{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [{ "name": "next" }],
    "baseUrl": "./src",
    "paths": {
      "pages-sections/*": ["pages-sections/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/myMap.js", "src/app/dashboard/api/auth/callback.js", "src/app/dashboard/api/auth/me/page.ts", "src/app/dashboard/api/auth/[auth0]/route.js", "pages-sections/vendor-dashboard/products/page-view", "components/flex-box", "components/Typography", "components/topbar"],
  "exclude": ["node_modules"]
}
